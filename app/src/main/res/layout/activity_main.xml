<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity"
    android:background="@drawable/bg">


    <LinearLayout
        android:id="@+id/linearLayout1"
        android:layout_width="match_parent"
        android:layout_height="97dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        android:layout_marginRight="10dp"
        android:alpha="0.75"
        android:background="@drawable/it"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="MissingConstraints">

        <LinearLayout
            android:layout_width="137dp"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="match_parent"
                android:layout_height="81dp"
                android:layout_marginTop="20px"
                android:layout_weight="1"
                app:srcCompat="@drawable/lg" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="274dp"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="247dp"
                android:layout_height="45dp"
                android:layout_marginTop="16dp"
                android:layout_marginLeft="30dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/login"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="25px"
                    android:layout_weight="1"
                    android:gravity="right"
                    android:fontFamily="@font/artifika"
                    android:text="LOGIN"
                    android:textColor="@color/colorRed"
                    android:textSize="15dp"
                    android:textStyle="bold" />

                <View
                    android:layout_width="2dp"
                    android:layout_height="20dp"
                    android:layout_marginTop="-1dp"
                    android:background="@color/colorRed">

                </View>

                <TextView
                    android:id="@+id/register"
                    android:layout_width="73dp"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="51px"
                    android:layout_weight="1"
                    android:paddingLeft="10dp"
                    android:text="REGISTER"
                    android:fontFamily="@font/artifika"
                    android:textColor="@color/colorRed"
                    android:textSize="15dp"
                    android:textStyle="bold" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="247dp"
                android:layout_height="51dp"
                android:layout_marginTop="-8dp"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/cart"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:paddingLeft="90dp"
                    app:srcCompat="@drawable/ic_cart"
                    tools:ignore="VectorDrawableCompat" />

                <ImageView
                    android:id="@+id/user"
                    android:layout_width="1dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"

                    app:srcCompat="@drawable/ic_person"
                    tools:ignore="VectorDrawableCompat" />

                <ImageView
                    android:id="@+id/menu"
                    android:layout_width="20dp"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="40px"
                    android:layout_weight="1"
                    app:srcCompat="@drawable/ic_menu"
                    tools:ignore="MissingPrefix,VectorDrawableCompat" />

            </LinearLayout>
        </LinearLayout>

    </LinearLayout>


    <ImageView
        android:id="@+id/home"
        android:layout_width="378dp"
        android:layout_height="180dp"
        android:layout_weight="1"

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.515"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.246"
        app:srcCompat="@drawable/home"
        tools:ignore="MissingPrefix,VectorDrawableCompat" />

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="372dp"
        android:layout_height="39dp"
        android:layout_marginEnd="8dp"
        android:alpha="0.75"
        android:background="@drawable/it"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints">

        <TextView
            android:id="@+id/shop_online"
            android:layout_width="38dp"
            android:layout_height="26dp"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="8dp"
            android:layout_weight="1"
            android:text="SHOP_ONLINE"
            android:textColor="@color/colorRed"
            android:textSize="14dp"
            android:textStyle="bold" />

        <View
            android:layout_width="3dp"
            android:layout_height="20dp"
            android:layout_marginTop="7dp"
            android:background="@color/colorRed">

        </View>

        <TextView
            android:id="@+id/specials"
            android:layout_width="29dp"
            android:layout_height="24dp"
            android:layout_marginTop="8dp"
            android:layout_weight="1"
            android:paddingLeft="13dp"
            android:text="SPECIALS"
            android:textColor="@color/colorRed"
            android:textSize="14dp"
            android:textStyle="bold" />

        <View
            android:layout_width="3dp"
            android:layout_height="20dp"
            android:layout_marginTop="7dp"
            android:layout_marginRight="10dp"
            android:background="@color/colorRed">

        </View>

        <TextView
            android:id="@+id/recent"
            android:layout_width="73dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:layout_weight="1"
            android:paddingLeft="1dp"
            android:text="RECENT ORDERS"
            android:textColor="@color/colorRed"
            android:textSize="14dp"
            android:textStyle="bold" />

    </LinearLayout>


    <Button
        android:id="@+id/button3"
        android:layout_width="152dp"
        android:layout_height="40dp"
        android:layout_marginLeft="5dp"
        android:layout_weight="1"
        android:fontFamily="@font/metal_mania"
        android:background="@drawable/cornerradius"
        android:text="HOME DELIVERY"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.16"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.593" />

    <Button
        android:id="@+id/button2"
        android:layout_width="152dp"
        android:layout_height="40dp"
        android:layout_weight="1"
        android:textColor="#FFFFFF"
        android:background="@drawable/cornerradius"
        android:text="WEEKLY CATALOGUE"
        android:fontFamily="@font/metal_mania"
        android:layout_marginRight="5dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.895"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.593" />

    <ImageView
        android:id="@+id/category"
        android:layout_width="378dp"
        android:layout_height="180dp"
        android:layout_weight="1"
        android:alpha="0.7"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.484"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.88"
        app:srcCompat="@drawable/catb"
        tools:ignore="MissingPrefix,VectorDrawableCompat" />

    <Button
        android:id="@+id/button4"
        android:layout_width="113dp"
        android:layout_height="23dp"
        android:layout_marginLeft="5dp"
        android:layout_weight="1"
        android:background="@drawable/click"
        android:text="CLICK HERE"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.895"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.685" />

    <TextView
        android:id="@+id/cat"
        android:layout_width="125dp"
        android:layout_height="40dp"
        android:layout_weight="1"
        android:gravity="right"
        android:text="CATEGORY"
        android:textColor="#000000"
        android:textSize="24dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.255"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/linearLayout1"
        app:layout_constraintVertical_bias="0.682"
        tools:ignore="MissingConstraints" />

    <LinearLayout
        android:layout_width="380dp"
        android:layout_height="39dp"
        android:alpha="0.75"
        android:background="@drawable/it"
        android:orientation="horizontal"
        app:layout_constraintBaseline_toBottomOf="@drawable/catb"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.516"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.991">

        <TextView
            android:id="@+id/about_us"
            android:layout_width="8dp"
            android:layout_height="26dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="8dp"
            android:layout_weight="1"
            android:text="ABOUT_US"
            android:textAlignment="center"
            android:textColor="#000000"
            android:textSize="14dp"
            android:textStyle="bold" />

        <View
            android:layout_width="3dp"
            android:layout_height="20dp"
            android:layout_marginTop="7dp"
            android:background="#000000">

        </View>

        <TextView
            android:id="@+id/Contact_us"
            android:layout_width="19dp"
            android:layout_height="24dp"
            android:layout_marginTop="8dp"
            android:layout_weight="1"
            android:paddingLeft="53dp"
            android:text="CONTACT_US"
            android:textAlignment="textStart"
            android:textColor="#000000"
            android:textSize="14dp"
            android:textStyle="bold" />
    </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>