<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".category"
    android:background="@drawable/bg">

    <LinearLayout
        android:id="@+id/linearLayout1"
        android:layout_width="match_parent"
        android:layout_height="97dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        android:layout_marginRight="10dp"
        android:alpha="0.75"
        android:background="@drawable/it"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="MissingConstraints">

        <LinearLayout
            android:layout_width="137dp"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="match_parent"
                android:layout_height="81dp"
                android:layout_marginTop="20px"
                android:layout_weight="1"
                app:srcCompat="@drawable/lg" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="274dp"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="247dp"
                android:layout_height="45dp"
                android:layout_marginTop="16dp"
                android:layout_marginLeft="30dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/login"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="25px"
                    android:layout_weight="1"
                    android:gravity="right"
                    android:fontFamily="@font/artifika"
                    android:text="LOGIN"
                    android:textColor="@color/colorRed"
                    android:textSize="15dp"
                    android:textStyle="bold" />

                <View
                    android:layout_width="2dp"
                    android:layout_height="20dp"
                    android:layout_marginTop="-1dp"
                    android:background="@color/colorRed">

                </View>

                <TextView
                    android:id="@+id/register"
                    android:layout_width="73dp"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="51px"
                    android:layout_weight="1"
                    android:paddingLeft="10dp"
                    android:text="REGISTER"
                    android:fontFamily="@font/artifika"
                    android:textColor="@color/colorRed"
                    android:textSize="15dp"
                    android:textStyle="bold" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="247dp"
                android:layout_height="51dp"
                android:layout_marginTop="-8dp"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/cart"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:paddingLeft="90dp"
                    app:srcCompat="@drawable/ic_cart"
                    tools:ignore="VectorDrawableCompat" />

                <ImageView
                    android:id="@+id/user"
                    android:layout_width="1dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"

                    app:srcCompat="@drawable/ic_person"
                    tools:ignore="VectorDrawableCompat" />

                <ImageView
                    android:id="@+id/menu"
                    android:layout_width="20dp"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="40px"
                    android:layout_weight="1"
                    app:srcCompat="@drawable/ic_menu"
                    tools:ignore="MissingPrefix,VectorDrawableCompat" />

            </LinearLayout>
        </LinearLayout>

    </LinearLayout>


    <ImageView
        android:id="@+id/imageView"
        android:layout_width="217dp"
        android:layout_height="120dp"
        android:layout_marginTop="4dp"
        android:alpha="0.7"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.208"
        app:srcCompat="@drawable/cheeze" />

    <Button
        android:id="@+id/chilled"
        android:layout_width="139dp"
        android:layout_height="42dp"
        android:background="@drawable/cornerradius"
        android:fontFamily="@font/cinzel_decorative"
        android:text="CHILLED FREEZER"
        android:textColor="#FFFFFF"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.246"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/health"
        android:layout_width="139dp"
        android:layout_height="42dp"
        android:background="@drawable/cornerradius"
        android:text="HEALTH AND BEAUTY"
        android:textColor="#FFFFFF"
        android:textStyle="bold"
        android:fontFamily="@font/cinzel_decorative"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.667"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/pooja"
        android:layout_width="139dp"
        android:layout_height="42dp"
        android:background="@drawable/cornerradius"
        android:text="POOJA ITEMS"
        android:textStyle="bold"
        android:fontFamily="@font/cinzel_decorative"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.909"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/snacks"
        android:layout_width="139dp"
        android:layout_height="42dp"
        android:background="@drawable/cornerradius"
        android:text="SNACKS"
        android:textStyle="bold"
        android:fontFamily="@font/cinzel_decorative"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.45"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="216dp"
        android:layout_height="107dp"
        android:alpha="0.7"
        android:textStyle="bold"
        android:fontFamily="@font/cinzel_decorative"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.438"
        app:srcCompat="@drawable/snacks"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="283dp"
        android:layout_height="138dp"
        android:alpha="0.7"
        android:layout_marginTop="5dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/linearLayout1"
        app:layout_constraintHorizontal_bias="0.555"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.678"
        app:srcCompat="@drawable/beayty" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="346dp"
        android:layout_height="143dp"
        android:alpha="0.7"
        android:layout_marginTop="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/linearLayout1"
        app:layout_constraintVertical_bias="0.974"
        app:srcCompat="@drawable/pooja" />


</androidx.constraintlayout.widget.ConstraintLayout>